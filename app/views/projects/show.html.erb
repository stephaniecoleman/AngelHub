<div class="row row-fluid homepage">
  <!-- Left Side Container -->
  <div class="col-xs-12">
  <!-- Requested Projects Container -->
   <div class="row row-fluid">
  <div class="col-xs-2"></div>
  <div class="col-xs-8">
    <div class="row row-fluid">
    <div class="col-xs-6">
         <h2 class="title"><%=@project.title %></h2>
         <p><%= @project.description %></p>
  </div>
  <div class="col-xs-6">
      <img src="<%= @project.project_pic.thumb %>"/>   
  </div> 
</div>
<div class="row row-fluid">
  <div class="col-xs-8">
    <h2>About Us </h2>
          <h3><strong><%= @project.organization.name %></strong></h3>
          <ul>
            <li> <%= link_to @project.organization.url %></li>
           <li><%= @project.organization.email %></li>
          <li><%= @project.organization.description %></li>
        </ul>
    </div>
</div>
<% if !@project.developers.empty?  %>
<div class="row row-fluid">
  <div class="col-xs-8">
    <h2>Developers on the Project</h2>
      <% @project.developers.each do |collab| %>
      <div class="col-xs-3">
        <img src="<%= collab.image %>"/>
      </div>
        <% end%>
    </div>
</div>
<% end%>
</div>
  <div class="col-xs-2"></div>
</div><!-- col -->
</div><!-- row -->